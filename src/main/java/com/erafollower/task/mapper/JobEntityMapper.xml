<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.erafollower.task.mapper.JobEntityMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.erafollower.task.model.po.JobEntity">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="group" property="group" />
        <result column="cron" property="cron" />
        <result column="parameter" property="parameter" />
        <result column="description" property="description" />
        <result column="vm_param" property="vmParam" />
        <result column="jar_path" property="jarPath" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, group, cron, parameter, description, vm_param, jar_path, status
    </sql>

</mapper>
